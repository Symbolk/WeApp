<view class="container flower_container" style="height:{{winHeight}}px;">
   <view class="flower-header-bg" background-size="contain" /> 
  <view class="flower-list-content">
    <view class="userinfo">
      <block>
        <image class="userinfo-avatar" src="{{userInfo.avatarUrl}}" background-size="cover"></image>
      </block>
    </view>
    <view class="msg">
      <text>谢谢您的帮助，{{userInfo.nickName}}!</text>
      <view>
        <text>您添加的信息已提交审核!</text>
      </view>
    </view>
  </view>
</view>